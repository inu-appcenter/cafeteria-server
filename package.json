{
  "name": "cafeteria-server",
  "main": "./index.mjs",
  "version": "1.8.0",
  "description": "Cafeteria API 서버입니다.",
  "scripts": {
    "test": "jest --coverage test/unit",
    "syncdb": "node -r esm actions/db/sync/index.mjs",
    "setupdb": "node -r esm actions/db/setup/index.mjs",
    "start": "node -r esm index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inu-appcenter/cafeteria-server.git"
  },
  "keywords": [
    "inu",
    "cafeteria"
  ],
  "author": "potados99",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/inu-appcenter/cafeteria-server/issues"
  },
  "homepage": "https://github.com/inu-appcenter/cafeteria-server#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "@hapi/good": "^9.0.1",
    "@hapi/good-console": "^8.1.2",
    "@hapi/good-squeeze": "^5.2.1",
    "@hapi/hapi": "^19.2.0",
    "@hapi/inert": "^6.0.3",
    "@hapi/joi": "^17.1.0",
    "@hapi/vision": "^6.0.1",
    "aws-sdk": "^2.818.0",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "bcrypt": "^5.0.0",
    "blipp": "^4.0.2",
    "cheerio": "^1.0.0-rc.5",
    "esm": "^3.2.25",
    "firebase-admin": "^9.4.2",
    "hapi-auth-jwt2": "^9.0.1",
    "hapi-swagger": "^12.1.1",
    "jsonwebtoken": "^8.5.1",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "qs": "^6.9.4",
    "rand-token": "^0.4.0",
    "sequelize": "^5.22.3",
    "stack-trace": "0.0.10",
    "stream": "0.0.2",
    "tough-cookie": "^4.0.0",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.5.2",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/tough-cookie": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-babel": "^5.3.1",
    "jest": "^26.6.3",
    "mockdate": "^3.0.2",
    "sequelize-mock": "^0.10.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "collectCoverageFrom": [
      "**/*.{mjs,js,}",
      "!index.mjs",
      "!config.mjs",
      "!test/**",
      "!lib/domain/constants/**",
      "!lib/domain/entities/**",
      "!lib/domain/repositories/**",
      "!lib/domain/security/**",
      "!lib/domain/usecases/**",
      "!lib/domain/validators/**",
      "!lib/infrastructure/database/**",
      "!lib/infrastructure/webserver/**",
      "!lib/infrastructure/uicoop/**",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "./node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60
      }
    }
  }
}
